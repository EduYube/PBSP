                        android:hint="0"
                        android:textColor="@color/green_pbsp"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </LinearLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/background_green"
                    android:gravity="center"
                    android:text="Inactividad tibial post"
                    android:textColor="@color/withe"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_IdaRodilla"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/cell_green"
                    android:gravity="center"
                    android:hint="0"
                    android:textColor="@color/green_pbsp"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_DchaRodilla"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/cell_green"
                    android:gravity="center"
                    android:hint="0"
                    android:textColor="@color/green_pbsp"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1.5">

                    <EditText
                        android:id="@+id/et_IdaAntecedentesRodilla"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:background="@drawable/cell_green"
                        android:gravity="center"
