                    android:layout_height="match_parent"
                    android:layout_weight="1.5">

                    <EditText
                        android:id="@+id/et_IdaAntecedentesTensionNeural"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:background="@drawable/cell_green"
                        android:gravity="center"
                        android:hint="0"
                        android:textColor="@color/green_pbsp"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/et_DchaAntecedentesTensionNeural"
