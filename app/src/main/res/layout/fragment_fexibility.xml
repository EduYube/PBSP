                    android:layout_height="match_parent"
                    android:layout_weight="1.5">

                    <EditText
                        android:id="@+id/et_IdaAntecedentesMecanismoWindlass"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:background="@drawable/cell_green"
                        android:gravity="center"
                        android:hint="0"
                        android:textColor="@color/green_pbsp"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/et_DchaAntecedentesMecanismoWindlass"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:background="@drawable/cell_green"
                        android:gravity="center"
                        android:hint="0"
                        android:textColor="@color/green_pbsp"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </LinearLayout>

            </TableRow>
        </TableLayout>

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/toArticulate_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:src="@drawable/ic_withe_arrow_left"
            android:backgroundTint="@color/blue_pbsp"
            app:layout_constraintTop_toBottomOf="@+id/tl_flexibility"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        
        <android.support.design.widget.FloatingActionButton
            android:id="@+id/toStrong_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:src="@drawable/ic_withe_arrow_rigth"
            app:backgroundTint="@color/red_pbsp"
            app:layout_constraintTop_toBottomOf="@+id/tl_flexibility"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </android.support.constraint.ConstraintLayout>
</layout>